<%= error_messages(f.object) %>
<table>
  <tr>
    <th>Customer Number</th>
    <td> <%= f.text_field:customerNumber %> </td>
  </tr>
      <th>Account Number</th>
      <td> <%= f.text_field:accountNumber, :readonly => true %> </td>
  </tr>
    <tr>
        <th>Account Name</th>
        <td> <%= f.text_field(:accountName) %> </td>
    </tr>
    <tr>
        <th>Balance</th>
        <td> <%= f.text_field:balance, :readonly => true %> </td>
    </tr>
    <tr>
        <th>Curreny</th>
        <td> <%= f.select :currency, [['EUR', 'EUR'],['GBP', 'GBP'],['USD', 'USD']]%> </td>
    </tr>

</table>
